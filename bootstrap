#!/bin/bash

Z=${BASH_SOURCE[0]}
echo "sourced $(basename $Z)"
########################
# Grab latest version  #
########################
DIRNAME=$( cd "$( dirname "$Z" )" && pwd )
MYVERSION=1

cd $DIRNAME
git stash
git checkout master
git pull origin master

echo $1
echo "-z" [ -z $1 ]
if [ -z $1 ]; then
  if [ -e "version.txt" ]; then
  echo "-e" [ -e "version.text" ] $(cat "version.txt")
    if [ $MYVERSION -gt $(cat "version.txt") ]; then
      echo $(basename $Z) $MYVERSION
      . $(basename $Z) $MYVERSION
    fi
  else
      echo "2" $(basename $Z) $MYVERSION
    . $(basename $Z) $MYVERSION
  fi
fi

. bash/setupaliases
. git/setupgit

cd $OLDPWD
